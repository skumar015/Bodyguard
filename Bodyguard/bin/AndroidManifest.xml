<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.apkabhaijaan"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="8"
        android:targetSdkVersion="18" />
    
     <permission
        android:name="com.example.apkabhaijaan.permission.MAPS_RECEIVE"
        android:protectionLevel="signature" />
   
<uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="com.google.android.providers.gsf.permission.READ_GSERVICES" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
 
    <!-- Required to show current location -->
    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION" />
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />
    <uses-permission android:name="android.permission.READ_CONTACTS"/>
    <uses-permission android:name="android.permission.CALL_PHONE"/>
    <uses-permission android:name="android.permission.SEND_SMS"/>

    <application
        android:allowBackup="true"
        android:icon="@drawable/logo"
        
        android:label="@string/app_name"
         
        >
        <activity
            android:name="com.example.apkabhaijaan.Splashscreen"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
       
         <activity
            android:name="com.example.apkabhaijaan.MainActivity"
            android:label="@string/app_name" />
         <activity
            android:name="com.example.apkabhaijaan.RegisterNext"
            android:label="@string/app_name" />
          <activity
            android:name="com.example.apkabhaijaan.Addcontacts"
            android:label="@string/app_name" />
           <activity
            android:name="com.example.apkabhaijaan.Safetytip"
            android:label="@string/app_name" />
            <activity
            android:name="com.example.apkabhaijaan.Trackme"
            android:label="@string/app_name" />
             <activity
            android:name="com.example.apkabhaijaan.Playalarm"
            android:label="@string/app_name" />
            
             <meta-data android:name="com.google.android.gms.version"
      android:value="@integer/google_play_services_version" />
          <meta-data
         android:name="com.google.android.maps.v2.API_KEY"
         android:value="AIzaSyBa0SwIa8V3hf1aumtkcjdKDA-M6dit5II" />
    </application>

</manifest>
